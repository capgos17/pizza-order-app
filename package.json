{
  "name": "pizzaorderapp",
  "version": "1.0.0",
  "description": "A pizza order app using NodeJS with real time status update using socket.io",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon server.js",
    "server": "node server.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capgos17/pizza-order-app.git"
  },
  "author": "ganesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/capgos17/pizza-order-app/issues"
  },
  "homepage": "https://github.com/capgos17/pizza-order-app#readme",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "mongoose": "^5.12.2",
    "nvm": "0.0.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "5.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.7",
    "resolve-url-loader": "^3.1.2",
